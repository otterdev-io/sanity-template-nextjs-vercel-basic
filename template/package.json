{
  "name": "simple-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "dev:all": "next dev --port $PORT & cd studio && sanity start",
    "build": "npm run build:sanity && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && npx sanity build ../public/studio -y && cd ..",
    "start:sanity": "cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@otterdev/next-sanity-extra": "^0.1.13",
    "@otterdev/sanity-react-extra": "^0.2.1",
    "autoprefixer": "^10.2.3",
    "lerna": "^3.22.1",
    "next": "10.0.0",
    "next-sanity": "^0.1.7",
    "next-seo": "^4.17.0",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-json-pretty": "^2.2.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "typescript": "^4.1.3"
  }
}
